{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"plugin:react/recommended",
		"airbnb",
		"prettier"
	],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"react"
	],
	"rules": {
		"jsx-a11y/label-has-associated-control": [
			"error",
			{
				"required": {
					"some": [
						"nesting",
						"id"
					]
				}
			}
		],
		"jsx-a11y/label-has-for": [
			"error",
			{
				"required": {
					"some": [
						"nesting",
						"id"
					]
				}
			}
		],
		"react/react-in-jsx-scope": "off"
	}
}